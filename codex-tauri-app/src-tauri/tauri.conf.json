{
  "productName": "Codex Account Manager",
  "version": "0.1.0",
  "identifier": "com.codex.account-manager",
  "build": {
    "beforeDevCommand": "npm run vite",
    "beforeBuildCommand": "vite build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
